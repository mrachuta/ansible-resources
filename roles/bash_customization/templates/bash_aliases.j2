#
# {{ ansible_managed }}
#

# Begin customizations for bash-completion
complete -F __start_kubectl k
complete -C /usr/bin/terraform terraform
complete -C /usr/bin/terraform tfm
complete -C /usr/bin/packer packer
complete -c /usr/bin/packer pkr

# End of customizations for bash-completion

# Begin of aliases list
# Aliases for tmux
alias tl="tmux ls"
alias td="tmux detach"
alias tk="tmux kill-session -t"
alias t1="tmux new-session -A -s t1"
alias t2="tmux new-session -A -s t2"
alias t3="tmux new-session -A -s t3"
alias t4="tmux new-session -A -s t4"
alias la="ls -all --block-size=k --color=auto --classify --human-readable"
alias ll="ls -all --block-size=k --color=auto --classify --human-readable"
alias lt="ls -all --block-size=k --color=auto --classify --human-readable --size -1 -S"
alias l="ls --block-size=k --color=auto"
alias ls="ls --block-size=k --color=auto"
alias snano="sudo nano"
alias svim="sudo vim"
alias ...="cd ../.."
alias ..="cd .."
alias shutdown="shutdown -h now"
alias sshutdown="sudo shutdown -h now"
alias reboot="shutdown -r now"
alias sreboot="sudo shutdown -r now"
alias scat="sudo cat"
alias syum="sudo yum"
alias sapt="sudo apt"
alias c="clear"
alias k="kubectl"
alias p="podman"
alias sp="sudo podman"
alias tfm="terraform"
alias pkr="packer"
alias kns="kubectl config set-context --current --namespace "
alias kctx="kubectl config use-context "
{% if bash_customization_bash_aliases_additional_entries is defined and bash_customization_bash_aliases_additional_entries is not none %}
{{ bash_customization_bash_aliases_additional_entries }}
{%  endif %}
# End of aliases list
